{
    "name": "voycey/cakephp-salesforce",
    "description": "Salesforce Enterprise CakePHP Datasource",
    "type": "cakephp-plugin",
    "homepage": "https://github.com/voycey/cakephp-salesforce",
    "license": "MIT",
    "require": {
        "php": ">=5.4.16",
        "developerforce/force.com-toolkit-for-php": "^v27.0.3"
    },
    "autoload": {
        "psr-4": {
            "Salesforce\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Salesforce\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
